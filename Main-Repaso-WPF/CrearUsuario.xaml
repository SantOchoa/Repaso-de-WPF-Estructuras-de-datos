<Window x:Class="Main_Repaso_WPF.CrearUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Main_Repaso_WPF"
        mc:Ignorable="d"
        Title="CrearUsuario" Height="450" Width="800">
    <Grid>
        <Button Name="createUser" Content="Button" HorizontalAlignment="Center" Height="54" Margin="0,269,0,0" VerticalAlignment="Top" Width="316" Click="createUser_Click"/>
        <TextBox Name ="name" HorizontalAlignment="Center" Height="38" Margin="0,100,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="228"/>
        <TextBox Name="id" HorizontalAlignment="Center" Height="38" Margin="0,200,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="228"/>
        <TextBox Name ="lastname" HorizontalAlignment="Center" Height="38" Margin="0,150,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="228" TextChanged="lastname_TextChanged"/>

    </Grid>
</Window>
